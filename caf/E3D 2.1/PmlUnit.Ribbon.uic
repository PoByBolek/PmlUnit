<?xml version="1.0" encoding="utf-8"?>
<UserInterfaceCustomization xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="www.aveva.com">
  <Version>1.0</Version>
  <Tools>
    <StateButtonTool Name="PmlUnit.ShowTestRunnerButton">
      <Command>
        <Type>Instance</Type>
        <Key>PmlUnit.ShowTestRunnerCommand</Key>
        <Arguments />
      </Command>
      <Image></Image>
      <ShortCut>CtrlT</ShortCut>
      <Caption>Test Runner</Caption>
      <DisplayStyle>Default</DisplayStyle>
    </StateButtonTool>
    <ButtonTool Name="PmlUnit.ShowSettingsDialogButton">
      <Command>
        <Type>Instance</Type>
        <Key>PmlUnit.ShowSettingsDialogCommand</Key>
        <Arguments />
      </Command>
      <ImageResourceId>AvevaSharedIcons:ID_DEFAULTS</ImageResourceId>
      <Caption>Settings</Caption>
      <DisplayStyle>Default</DisplayStyle>
    </ButtonTool>
    <ButtonTool Name="PmlUnit.ShowAboutDialogButton">
      <Command>
        <Type>Instance</Type>
        <Key>PmlUnit.ShowAboutDialogCommand</Key>
        <Arguments />
      </Command>
      <ImageResourceId>AvevaSharedIcons:ID_HELP</ImageResourceId>
      <Caption>About</Caption>
      <DisplayStyle>Default</DisplayStyle>
    </ButtonTool>
  </Tools>
  <InstanceTools />
  <MenuBar />
  <CommandBars />
  <TaskPanes />
  <ContextMenus />
  <AreaLeftTools />
  <AreaRightTools />
  <FooterTools />
  <NavigationMenuTools />
  <QATTools />
  <ContextualTabGroups />
  <TabToolbarTools />
  <Tabs>
    <Tab Name="PmlUnit.MainTab">
      <Caption>PML UNIT</Caption>
      <ContextualTabGroup>
        <Item>None</Item>
      </ContextualTabGroup>
      <Groups>
        <Group Name="PmlUnit.MainGroup">
          <Tools>
            <Tool Name="PmlUnit.ShowTestRunnerButton" LargeImage="True" ImageOnly="False" />
          </Tools>
          <Caption>Run</Caption>
        </Group>
        <Group Name="PmlUnit.HelpGroup">
          <Tools>
            <Tool Name="PmlUnit.ShowSettingsDialogButton" LargeImage="True" ImageOnly="False" />
            <Tool Name="PmlUnit.ShowAboutDialogButton" />
          </Tools>
          <Caption>Help</Caption>
        </Group>
      </Groups>
      <KeyTip>U</KeyTip>
    </Tab>
  </Tabs>
  <MiniToolbarTools />
  <Namespace>PmlUnit</Namespace>
</UserInterfaceCustomization>
